{
    "version": 2,
    "builds": [
        { 
            "src": "PdfReader_FYP/Back-end/index.js", 
            "use": "@vercel/node" 
        }
    ],
    "routes": [
        { 
            "src": "/(.*)", 
            "dest": "PdfReader_FYP/Back-end/index.js", 
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"] 
        }
    ]
}


